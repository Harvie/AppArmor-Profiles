#include <tunables/global>

/usr/bin/man flags=(complain) {
  #include <abstractions/base>
  #include <abstractions/bash>
  #include <abstractions/nameservice>
  #include <abstractions/user-manpages>

  capability setgid,
  capability setuid,



  /bin/less rix,
  /etc/man_db.conf r,
  /opt/java/jre/man/ r,
  /opt/java/jre/man/* rk,
  /opt/java/man/ r,
  /opt/java/man/* rk,
  /opt/java/man/*/ r,
  /opt/kde/man/ r,
  /opt/kde/man/*/ r,
  /opt/qt/man/ r,
  /opt/qt/man/* rk,
  /opt/qt/man/*/ r,
  /root/.lesshst w,
  /usr/bin/nroff rix,
  /usr/bin/preconv rix,
  /usr/bin/tbl rix,
  /usr/lib/man-db/man Px,
  /usr/local/man/ r,
  /usr/man/ r,
  /usr/share/man/ r,
  /var/cache/man/** rk,

}
